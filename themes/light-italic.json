{
  "name": "Sugar Light Italic",
  "$schema": "vscode://schemas/color-theme",
  "include": "./light.json",
  "tokenColors": [
    {
      "scope": [
        "storage.type",
        "keyword.control",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression",
        "support.type.object.module.js",
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "storage.type.function.arrow",
        "keyword.control.anchor.regexp"
      ],
      "settings": {
        "fontStyle": ""
      }
    }
  ]
}
