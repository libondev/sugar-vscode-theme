    "peekViewEditor.matchHighlightBackground": "#05966955",
    "peekViewResult.matchHighlightBackground": "#05966955",
    "pickerGroup.border": "#3C3C3C",
    "ports.iconRunningProcessForeground": "#059669",
    "progressBar.background": "#059669",
    "quickInput.background": "#222222",
    "quickInput.foreground": "#AAAAAA",
    // list selected bg
    "quickInputList.focusBackground": "#05966988",
    "settings.dropdownBackground": "#313131",
    "settings.dropdownBorder": "#3C3C3C",
    "settings.headerForeground": "#EEEEEE",
    "settings.modifiedItemIndicator": "#059669aa",
    "sideBar.border": "#222",
    "sideBar.background": "#191919",
    "sideBar.foreground": "#AAAAAA",
    "sideBarSectionHeader.border": "#0000",
    "sideBarSectionHeader.background": "#0000",
    "sideBarSectionHeader.foreground": "#AAAAAA",
    "sideBarTitle.foreground": "#AAAAAA",
    "statusBar.background": "#191919",
    "statusBar.border": "#222",
    "statusBar.debuggingBackground": "#059669",
    "statusBar.debuggingForeground": "#EEEEEE",
    "statusBar.focusBorder": "#059669",
    "statusBar.foreground": "#AAAAAA",
    "statusBar.noFolderBackground": "#1F1F1F",
    "statusBarItem.focusBorder": "#059669",
