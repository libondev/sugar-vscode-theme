    "welcomePage.progress.foreground": "#059669",
    "welcomePage.tileBackground": "#0000",
    "widget.border": "#313131"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown",
        "variable.legacy.builtin.python"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#000080"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "variable.other.enummember",
        "meta.preprocessor.numeric",
        "keyword.operator.plus.exponent",
        "keyword.operator.minus.exponent",
        "storage.type.numeric.bigint"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#4C9A91"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      // html tag name / css tag selector
      "scope": [
        "entity.name.tag",
        "entity.name.tag.css",
        "entity.name.tag.less",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "source.css entity.other.attribute-name.class",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.parent.less",
        "source.css entity.other.attribute-name.pseudo-class",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      // deprecated property name
      "scope": [
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "fontStyle": "strikethrough",
        "foreground": "#B8A965"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F85149"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#144620",
        "foreground": "#85e89d"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#86181d",
        "foreground": "#fdaeb7"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#c24e00",
        "foreground": "#ffab70"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#2f363d",
        "background": "#79b8ff"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#b392f0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#6A9955"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#79b8ff"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#79b8ff"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#79b8ff"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "support.type.builtin",
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#cb7676"
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#53a883"
      }
    },
    {
      "scope": [
        "string",
        "meta.embedded.assembly",
        "meta.attribute-selector" // css property selector value
      ],
      "settings": {
        "foreground": "#C98A7D"
      }
    },

    {
      // html attribute name / css property name
      "scope": [
        "entity.other.attribute-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#B8A965"
      }
    },
    {
      // json/object property
      "scope": [
        "support.type.vendored.property-name",
        "source.coffee.embedded",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      // css property name
      "scope": [
        "meta.property-name.css",
        "meta.property-name.scss",
        "meta.property-list.less",
        "source.css variable",
        "support.type.property-name.css",
        "support.type.property-name.less",
        "punctuation.definition.custom-property.less"
      ],
      "settings": {
        "foreground": "#B8A965"
      }
    },
    {
      "scope": [
        // "keyword.control",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.alignof",
        "keyword.operator.typeid",
        "keyword.operator.alignas",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python",
        "keyword.operator.wordlike",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      // export/function/class/const ...
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.control.as",
        "keyword.control.loop",
        "keyword.control.flow",
        "keyword.control.from",
        "keyword.control.type",
        "keyword.control.export",
        "keyword.control.import",
        "keyword.control.conditional",
        "keyword.operator.new",
        "keyword.operator.noexcept",
        "keyword.operator.expression",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
      ],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      // class
      "scope": [
        "entity.name.type",
        "support.type.primitive",
        "entity.other.inherited-class", // e.g. React.Component

        // vue language support
        "keyword.control.conditional.vue",
        "entity.other.attribute-name.html.vue"
      ],
      "settings": {
        "foreground": "#14b8a6"
      }
    },
    {
      // constant variable
      "name": "Variable and parameter name",
      "scope": [
        "variable",
        "variable.language", // this.self
        "meta.definition.variable.name",

        // "support.variable",
        "entity.name.variable",
        "constant.other.placeholder" // placeholders in strings
      ],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#CE9178"
      }
    },
    {
      "name": "Regular expression groups",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "keyword.operator.negation.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": {
        "foreground": "#CE9178CC"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#5889d6"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#C98A7D"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#e6cc77"
      }
    },
    {
      "scope": [
        "punctuation",
        "meta.brace.round", // ()
        "meta.brace.square", // {}
        "storage.type.function.arrow",
        "keyword.operator.assignment",
        "keyword.operator.spread",
        "keyword.operator.rest",
        "keyword.operator.scss",
        "keyword.operator.pattern.css",
        "keyword.operator.attribute-selector.less",
        "keyword.operator.type.annotation"
        // "punctuation.definition.block",
        // "punctuation.definition.tag",
        // "punctuation.definition.list",
        // "punctuation.definition.attribute-selector",
        // "punctuation.definition.parameters",
        // "punctuation.section.property-list",
        // "punctuation.definition.arguments",
        // "punctuation.terminator.statement",
        // "punctuation.separator.comma",

        // json property/value quote
        // "punctuation.support.type.property-name",
        // "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#8888885f"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "comment": "#5C6370",
    "namespace": "#67abbd",
    "interface": "#67abbd",
    // "property": "#c5b570",
    // "class": "#8592de",
    "type": "#67abbd"
  }
}
