    "tab.lastPinnedBorder": "#0000",
    "tab.unfocusedActiveBorder": "#222",
    "tab.unfocusedActiveBorderTop": "#2B2B2B",
    "tab.unfocusedHoverBackground": "#222",
    "terminal.foreground": "#AAAAAA",
    "terminal.inactiveSelectionBackground": "#3A3D41",
    "terminal.tab.activeBorder": "#059669",
    "textBlockQuote.background": "#2B2B2B",
    "textBlockQuote.border": "#616161",
    "textCodeBlock.background": "#2B2B2B",
    "textLink.activeForeground": "#059669",
    "textLink.foreground": "#059669",
    "textPreformat.background": "#3C3C3C",
    "textPreformat.foreground": "#D0D0D0",
    "textSeparator.foreground": "#21262D",
    "titleBar.activeBackground": "#191919",
