{
  "name": "Sugar Dark Stylization",
  "$schema": "vscode://schemas/color-theme",
  "include": "./dark.json",
  "tokenColors": [
    {
      "scope": [
        "meta.brace",
        "punctuation",
        "meta.parameters",
        "keyword.operator",
        "meta.array.literal",
        "meta.embedded.block",
        "meta.attribute.directive",
        "meta.function.expression",
        "storage.type.function.arrow",
        "punctuation.section.function",
        "punctuation.definition.entity",
        "punctuation.separator.key-value",
        "punctuation.definition.parameters",
        "punctuation.definition.typeparameters",
        "punctuation.definition.attribute-selector",
        "source.css.less punctuation.definition.group",
        "invalid.illegal.character-not-allowed-here.html",
      ],
      "settings": {
        "foreground": "#7777778F"
      }
    },
    {
      "scope": [
        "punctuation.accessor", // xxx(.)xxx
        "keyword.operator.ternary", // ? :
        "keyword.operator.logical",
        "keyword.operator.bitwise",
        "keyword.operator.increment",
        "keyword.operator.decrement",
        "keyword.operator.relational",
        "keyword.operator.comparison",
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "invalid.illegal.combinator.less",
      ],
      "settings": {
        "foreground": "#aaa"
      }
    },
  ]
}
