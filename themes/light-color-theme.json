	"tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#999",
      }
    },
    {
      "scope": [
        "variable",
        "identifier",
        "support.constant",
				"support.variable",
				"entity.name.variable",
        "meta.object-literal.key",
				"constant.other.placeholder",
				"meta.definition.variable.name",
        "variable.other.constant.property",
      ],
      "settings": {
        "foreground": "#2786be",
      }
    },
    {
      "scope": [
        "support.variable",
        "variable.language.this",
        "variable.other.constant",
				"variable.other.enummember",
        "variable.other.readwrite.alias",
      ],
      "settings": {
        "foreground": "#9042be",
      }
    },
    {
      "scope":[
        "string",
        "meta.embedded.assembly",
				"meta.attribute-selector.scss"
      ],
      "settings": {
        "foreground": "#448c27",
      }
    },
    {
      "scope": [
        "constant.language", // undefined/null/nil
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#059669",
      }
    },
    {
      "scope": [
        "number",
        "constant.numeric", // 1234567890
        "storage.type.numeric.bigint", // bigint: n
        "keyword.operator.quantifier.regexp",
      ],
      "settings": {
        "foreground": "#76a35e",
        "fontStyle": ""
      }
    },
    {
      // reset to default color
      "scope": [
        "keyword.operator", // .../||/&&/??/!/|/&/^/++/--/==/!=/></>=<=
        "storage.type.function.arrow", // =>
        "punctuation.separator.key-value", // object key:
        // scss ([) xxx=xxx (])
        "punctuation.definition.attribute-selector.begin.bracket.square.scss",
				"punctuation.definition.attribute-selector.end.bracket.square.scss",
      ],
      "settings": {
        "foreground": "#555555",
      }
    },
    {
      // property name
      "scope": [
        // html attribute name
        "entity.other.attribute-name",
        // html deprecated attribute: border/align
        "invalid.deprecated.entity.other.attribute-name.html",
        // less custom property: --xxx
        "support.type.custom-property.less",
        "entity.name.function.scss", // scss @keyframe (Animate)
        "meta.at-rule.keyframe.less",
        "support.type.property-name.media", // @media ((min-width): 100px)
      ],
      "settings": {
        "foreground": "#1580c7",
      }
    },
    {
      // css property name
			"scope": [
				"source.coffee.embedded",
				"support.type.property-name.css",
				"support.type.property-name.less",
        "support.type.vendor-prefix.less", // -webkit-/-moz-/-ms-/-o-
        "support.type.vendored.property-name.css",

        // css keyframe
        "entity.other.keyframe-offset",
        "entity.other.attribute-name.scss",
        "keyword.other.keyframe-selector.less",
			],
			"settings": {
				"foreground": "#5d69c4"
			}
    },
    {
      "scope": [
        // undefined/null
        "constant.language.null",
        "constant.language.undefined",
        "keyword.other.important",
      ],
      "settings": {
        "foreground": "#c73939",
      }
    },
    {
      "scope": [
        // html tag name
        "entity.name.tag",
        // json property name
        "string.json.comments",
        "support.class.component", // jsx class component
      ],
      "settings": {
        "foreground": "#d33a3d",
      }
    },
    {
      // language keywords
      "scope": [
        "keyword",
        // const/var/let/function/interface/enum/class/(type Aa = 1)/constructor/get/set
        "storage.type",
        // import/export/(import type Aa)/return/if/else/for/while/switch/try/catch/finally/throw
        "keyword.control",
        // extends/class readonly/class private/class public
        "storage.modifier",
        // new
        "keyword.operator.new",
        // delete/void/typeof/instanceof/in
        "keyword.operator.expression",
        // module.exports
        "support.type.object.module.js",
        // template expression: ${}
        "punctuation.definition.template-expression",
      ],
      "settings": {
        "foreground": "#d33a3d",
      }
    },
    {
      "scope": [
        "type.identifier",
        "constant.other.character-class.regexp"
      ],
      "settings": {
        "foreground": "#5a6aa6"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#c4704f"
      }
    },
    {
      "scope": [
        "keyword.operator.or.regexp", // |
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#c98a7d"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#e6cc77"
      }
    },
    {
      "scope": [
        // hover card function (name)
        "entity.name",
        "entity.name.function", // function name
        "support.function.url", // (url)()
        "support.function.misc", // (var)(--var)
        "support.function.format",
        "support.function.var.less", // less css
        "support.function.filter.less", // less (attr)()
      ],
      "settings": {
        "foreground": "#2d69d7",
      }
    },
    {
      "scope": [
        "entity.name.type.class", // class (Name)
        "entity.name.type.module", // extends (React).Component
        "entity.other.inherited-class", // class Name extends (Test)
      ],
      "settings": {
        "foreground": "#d03917",
      }
    },
    {
      "scope": [
				"entity.name.type",
				"entity.name.type.parameter", // TS generic type
				// ts function params type
				"meta.function,meta.type.annotation",
				"entity.name.type.module,meta.namespace.declaration",
      ],
      "settings": {
        "foreground": "#0db2d0",
      }
    },
    {
      "scope": [
				// string/number/boolean/symbol
        "support.type.primitive",
      ],
      "settings": {
        "foreground": "#05c2c2",
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#059669",
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#5d99a9"
      }
    },
    {
      // link/image
      "scope": [
        "markup.underline.link",
        "variable.other.link.underline",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#33a07cd1"
      }
    },

    // git diff
    {
			"scope": [
        "markup.inserted",
        // "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
			"settings": {
				"foreground": "#098658"
			}
		},
		{
			"scope": [
        "markup.deleted",
        // "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
			"settings": {
				"foreground": "#a31515"
			}
		},
		{
			"scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
			"settings": {
				"foreground": "#046a4a"
			}
		},
  ]
}
