      "settings": {
        "foreground": "#8FC1FF"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html.vue"
      ],
      "settings": {
        "foreground": "#45D174"
      }
    },
    {
      "scope": [
        "invalid.illegal.unrecognized-tag.html"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    }
