  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#53E0ED",
    "property": "#F7CC2F",
    "interface": "#53E0ED",
    "type": "#53E0ED",
    "class": "#8FC1FF"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#aaa"
      }
    },
    {
      "scope": [
        "delimiter.bracket",
        "delimiter",
        "invalid.illegal.character-not-allowed-here.html",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type.annotation",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "meta.brace",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "meta.tag.structure.input.void.html",
        "meta.type.annotation",
        "meta.embedded.block.github-actions-expression",
        "storage.type.function.arrow",
        "keyword.operator.type",
        "meta.objectliteral.ts",
        "punctuation"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#8888886f"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#FFB169"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression.delete"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#45D174"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#393a34"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "tag.html"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#45D174"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type.class.jsdoc"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": [
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": [
        "text.html.derivative",
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#393a34"
      }
    },
    {
      "scope": [
        "string",
        "string punctuation.section.embedded source",
        "attribute.value"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#F7CC2F"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#8888886f"
      }
    },
    {
      "scope": [
        "property",
        "meta.property-name",
        "meta.object-literal.key",
        "entity.name.tag.yaml",
        "attribute.name"
      ],
      "settings": {
        "foreground": "#F7CC2F"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#F7CC2F"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#F7CC2F"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": "namespace",
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "meta.var.expr.ts"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A31D26"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A31D26"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A31D26"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A31D26"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#EF4444",
        "foreground": "#FFFFFF",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#A31D26"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#EC4899"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#008EAD"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#FFB169"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "number"
      ],
      "settings": {
        "foreground": "#2BCBE0"
      }
    },
    {
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#EF72FC"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language"
      ],
      "settings": {
        "foreground": "#06B6D4"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#139E4B"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#139E4B"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#393a34"
      }
    },
    {
      "scope": "markup.strikethrough.markdown",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#393a34"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#393a34"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#139E4B"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#FFF2F0",
        "foreground": "#A31D26"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#F0FFF3",
        "foreground": "#139E4B"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#FFE0BA",
        "foreground": "#D45608"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#FAFAFA",
        "background": "#2762CF"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#843DD1",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#2762CF"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#2762CF"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#2762CF"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
