	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				// "fontStyle": "italic",
				"foreground": "#555"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
        // css property selector value
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#705042"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"identifier",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#6e5b41"
			}
		},
		{
			"name": "Quoted",
      "scope": [
				"punctuation.definition.string",
				"punctuation.support.type.property-name.begin.json.comments",
				"punctuation.support.type.property-name.end.json.comments"
			],
      "settings": {
        "foreground": "#bd976a66"
      }
    },
		{
      "scope": [
        "entity.name.type",
        "support.type.primitive",
				// vue language support
				"keyword.control.conditional.vue",
				"entity.other.attribute-name.html.vue"
      ],
      "settings": {
        "foreground": "#3e6d5e"
      }
    },
		{
			"name": "Colors",
			"scope": [
