{
	"name": "Sugar Dark Dimmed",
	"include": "./sugar-dark-classic-color-theme.json",
	"colors": {
		"activityBar.activeBorder": "#3D725B",
		"activityBar.background": "#000000",
		"activityBar.border": "#242424",
		"activityBarBadge.background": "#3D725B",
		"activityBarBadge.foreground": "#DFDFDF",
		"button.background": "#3D725B",
		"button.secondaryForeground": "#9F9F9F",
		"chat.slashCommandForeground": "#3D725B",
		"debugToolBar.background": "#000000",
		"dropdown.foreground": "#9F9F9F",
		"dropdown.listBackground": "#000000",
		"editor.background": "#000000",
		"editor.foreground": "#666666",
		"editorGroupHeader.tabsBackground": "#000000",
		"editorGroupHeader.tabsBorder": "#242424",
		"editorGutter.modifiedBackground": "#3D725B",
		"editorLineNumber.activeForeground": "#9F9F9F",
		"focusBorder": "#0000",
		"foreground": "#9F9F9F",
		"icon.foreground": "#9F9F9F",
		"input.foreground": "#9F9F9F",
		"inputOption.activeBackground": "#3D725B82",
		"inputOption.activeBorder": "#3D725B",
		"keybindingLabel.foreground": "#9F9F9F",
		"list.activeSelectionBackground": "#3D725B82",
		"menu.background": "#000000",
		"menu.border": "#3C3C3C",
		"menu.foreground": "#9F9F9F",
		"menu.selectionBackground": "#3D725B",
		"notificationCenterHeader.background": "#000000",
		"notificationCenterHeader.foreground": "#9F9F9F",
		"notifications.background": "#000000",
		"notifications.border": "#242424",
		"notifications.foreground": "#9F9F9F",
		"panel.background": "#000000",
		"panel.border": "#242424",
		"panelInput.border": "#242424",
		"panelTitle.activeBorder": "#3D725B",
		"panelTitle.activeForeground": "#9F9F9F",
		"peekViewEditor.background": "#000000",
		"peekViewResult.background": "#000000",
		"progressBar.background": "#3D725B",
		"quickInput.foreground": "#9F9F9F",
		"sideBar.background": "#000000",
		"sideBar.border": "#242424",
		"sideBar.foreground": "#9F9F9F",
		"sideBarSectionHeader.border": "#0000",
		"sideBarSectionHeader.foreground": "#9F9F9F",
		"sideBarTitle.foreground": "#9F9F9F",
		"statusBar.background": "#000000",
		"statusBar.border": "#242424",
		"statusBar.debuggingBackground": "#3D725B",
		"statusBar.focusBorder": "#3D725B",
		"statusBar.foreground": "#9F9F9F",
		"statusBar.noFolderBackground": "#000000",
		"statusBarItem.focusBorder": "#3D725B",
		"statusBarItem.remoteBackground": "#3D725B",
		"tab.activeBackground": "#000000",
		"tab.activeBorder": "#000000",
		"tab.activeBorderTop": "#3D725B",
		"tab.activeForeground": "#BBBBBB",
		"tab.border": "#242424",
		"tab.hoverBackground": "#000000",
		"tab.inactiveBackground": "#000000",
		"tab.unfocusedActiveBorder": "#000000",
		"tab.unfocusedActiveBorderTop": "#242424",
		"tab.unfocusedHoverBackground": "#000000",
		"terminal.foreground": "#9F9F9F",
		"terminal.tab.activeBorder": "#3D725B",
		"textBlockQuote.background": "#242424",
		"textCodeBlock.background": "#242424",
		"textLink.activeForeground": "#3D725B",
		"textLink.foreground": "#3D725B",
		"titleBar.activeBackground": "#000000",
		"titleBar.activeForeground": "#9F9F9F",
		"titleBar.border": "#242424",
		"titleBar.inactiveBackground": "#000000",
		"welcomePage.progress.foreground": "#3D725B",
		"welcomePage.tileBackground": "#242424",
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#5F5F5F"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"variable.other.enummember",
				"keyword.operator.plus.exponent",
				"keyword.operator.minus.exponent"
			],
			"settings": {
				"foreground": "#75856c"
			}
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#9f434b"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name.class.css",
				"source.css entity.other.attribute-name.class",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.parent-selector.css",
				"entity.other.attribute-name.parent.less",
				"source.css entity.other.attribute-name.pseudo-class",
				"entity.other.attribute-name.pseudo-element.css",
				"source.css.less entity.other.attribute-name.id",
				"entity.other.attribute-name.scss"
			],
			"settings": {
				"foreground": "#a7905e"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#75856C"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#9C6C59"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#9C6C59"
			}
		},
		{
			"name": "brackets of XML/HTML tags",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.separator.key-value",
				"punctuation.definition.string",
				"punctuation.attribute-shorthand.bind.html.vue",
				"punctuation.attribute-shorthand.event.html.vue",
				"punctuation.definition.interpolation.begin.html.vue",
				"punctuation.definition.interpolation.end.html.vue",
			],
			"settings": {
				"foreground": "#444444"
			}
		},
		{
			"scope": [
				"meta.preprocessor",
				"entity.name.function.preprocessor"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "meta.preprocessor.string",
			"settings": {
				"foreground": "#9C6C59"
			}
		},
		{
			"scope": "meta.preprocessor.numeric",
			"settings": {
				"foreground": "#75856C"
			}
		},
		{
			"scope": "meta.structure.dictionary.key.python",
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"scope": "meta.diff.header",
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": [
				"storage.type",
				"keyword.control",
				"keyword.operator.new",
				"keyword.operator.expression",
			],
			"settings": {
				"foreground": "#a15b5b"
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.operator.noexcept"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": [
				"string",
				"meta.embedded.assembly"
			],
			"settings": {
				"foreground": "#6c7b39"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": [
				"source.css variable",
				"source.coffee.embedded"
			],
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"scope": [
				"keyword.operator.cast",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.typeid",
				"keyword.operator.alignas",
				"keyword.operator.instanceof",
				"keyword.operator.logical.python",
				"keyword.operator.wordlike"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"name": "Control flow / Special keywords",
			"scope": [
				"keyword.other.using",
				"keyword.other.directive.using",
				"keyword.other.operator",
				"entity.name.operator"
			],
			"settings": {
				"foreground": "#3d725b"
			}
		},
		{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#75856C"
			}
		},
		{
			"scope": [
				"punctuation.section.embedded.begin.php",
				"punctuation.section.embedded.end.php"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "support.function.git-rebase",
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"scope": "constant.sha.git-rebase",
			"settings": {
				"foreground": "#75856C"
			}
		},
		{
			"name": "this.self",
			"scope": "variable.language",
			"settings": {
				"foreground": "#3D725B"
			}
		},

		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.function",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal" // See https://en.cppreference.com/w/cpp/language/user_literal
			],
			"settings": {
				"foreground": "#688652"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"support.class",
				"support.type",
				"entity.name.type",
				"entity.name.namespace",
				"entity.other.attribute",
				"entity.name.scope-resolution",
				"entity.name.class",
				"storage.type.numeric.go",
				"storage.type.byte.go",
				"storage.type.boolean.go",
				"storage.type.string.go",
				"storage.type.uintptr.go",
				"storage.type.error.go",
				"storage.type.rune.go",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#5762a1"
			}
		},
		{
			"name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class"
			],
			"settings": {
				"foreground": "#5762a1"
			}
		},
		{
			// jsx children
			"scope": [
				"meta.jsx.children",
				"text.html.derivative"
			],
			"settings": {
				"foreground": "#888888"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable",
				"meta.definition.variable.name",
				"support.variable",
				"entity.name.variable",
				"constant.other.placeholder", // placeholders in strings
			],
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"name": "Constants and enums",
			"scope": [
				"variable.other.constant",
				"variable.other.enummember"
			],
			"settings": {
				"foreground": "#967954",
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#967954"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"support.constant.media",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color"
			],
			"settings": {
				"foreground": "#9C6C59"
			}
		},
		{
			"scope": [
				"source.css variable",
				"source.coffee.embedded",
				"support.type.property-name.css",
				"support.type.property-name.less",
				"support.type.custom-property.name.less",
				"punctuation.definition.custom-property.less"
			],
			"settings": {
				"foreground": "#8e824e"
			}
		},
		{
			"name": "Regular expression groups",
			"scope": [
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#9C6C59"
			}
		},
		{
			"scope": [
				"keyword.operator.or.regexp",
				"keyword.control.anchor.regexp"
			],
			"settings": {
				"foreground": "#688652"
			}
		},
		{
			"scope": "keyword.operator.quantifier.regexp",
			"settings": {
				"foreground": "#a7905e"
			}
		},
		{
			"scope": [
				"constant.character",
				"constant.other.option"
			],
			"settings": {
				"foreground": "#3D725B"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#a7905e"
			}
		},
	]
}
