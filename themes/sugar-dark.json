  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#53E0ED",
    "property": "#F7CC2Fdd",
    "interface": "#53E0ED",
    "type": "#53E0ED",
    "class": "#66A6FF"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#666"
      }
    },
    {
      "scope": [
        "delimiter.bracket",
        "delimiter",
        "invalid.illegal.character-not-allowed-here.html",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type.annotation",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "meta.brace",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "meta.tag.structure.input.void.html",
        "meta.type.annotation",
        "meta.embedded.block.github-actions-expression",
        "storage.type.function.arrow",
        "keyword.operator.type",
        "meta.objectliteral.ts",
        "punctuation"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#8888886f"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#FFB169"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression.delete"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#6DDE8F"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#dbd7caee"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "tag.html"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#6DDE8F"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type.class.jsdoc"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "support.type.builtin"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": [
        "constant.language.undefined",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": [
        "text.html.derivative",
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#dbd7caee"
      }
    },
    {
      "scope": [
        "string",
        "string punctuation.section.embedded source",
        "attribute.value"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#F7CC2Fdd"
      }
    },
    {
      "scope": [
        "punctuation.definition.string",
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#8888886f"
      }
    },
    {
      "scope": [
        "property",
        "meta.property-name",
        "meta.object-literal.key",
        "entity.name.tag.yaml",
        "attribute.name"
      ],
      "settings": {
        "foreground": "#F7CC2Fdd"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#F7CC2Fdd"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#F7CC2Fdd"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": "namespace",
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "meta.var.expr.ts"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#FFA099",
        "foreground": "#000000",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#EC4899"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#FFB169"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "number"
      ],
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": [
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#FF6B7F"
      }
    },
    {
      "scope": [
        "constant.language.boolean",
        "constant.language"
      ],
      "settings": {
        "foreground": "#2BCBE0"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#22C55E"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#53E0ED"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dbd7caee"
      }
    },
    {
      "scope": "markup.strikethrough.markdown",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline",
        "foreground": "#dbd7caee"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#dbd7caee"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#570A15",
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#002B16",
        "foreground": "#6DDE8F"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#872B00",
        "foreground": "#FFB169"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#141414",
        "background": "#8FC1FF"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#C680FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#8FC1FF"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#8FC1FF"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#8FC1FF"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#8C8C8C"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#FFC8C2"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link",
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown"
      ],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": [
        "markup.underline.link.markdown",
        "markup.underline.link.image.markdown"
      ],
      "settings": {
        "foreground": "#dedcd590",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "type.identifier"
      ],
      "settings": {
        "foreground": "#66A6FF"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html.vue"
      ],
      "settings": {
        "foreground": "#6DDE8F"
      }
    },
    {
      "scope": [
        "invalid.illegal.unrecognized-tag.html"
      ],
      "settings": {
        "fontStyle": "normal"
      }
    }
  ]
}
