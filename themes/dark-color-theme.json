  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": [
        "comment",
        "string.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": [
        "support",
        "property",
        "variable",
        "identifier",
        "attribute.name",
        "meta.property-name",
        "meta.object-literal.key",
      ],
      "settings": {
        "foreground": "#b8a965"
      }
    },
    {
      "scope": [
        "support.variable", // (console).log
        "variable.language", // (this), (this).name
      ],
      "settings": {
        "foreground": "#c5975e"
      }
    },
    {
      "scope": [
        "constant", // /[(123)|(123)]/
        "entity.name.constant",
        "meta.definition.variable",
      ],
      "settings": {
        "foreground":"#c99076"
      }
    },
    {
      "scope":[
        "string",
        "meta.embedded.assembly",
				"meta.attribute-selector.scss",
        "string punctuation.section.embedded source",
      ],
      "settings": {
        "foreground": "#97b579"
      }
    },
    {
      "scope": [
        "constant.language",
        "support.type.builtin",
        "meta.module-reference",
      ],
      "settings": {
        "foreground": "#5DA994"
      }
    },
    {
      "scope": [
        "number",
        "constant.numeric", // 1234567890
        "keyword.other.unit", // 1(em), 10(%)
        // css keyframe (from) (to) (100%)
        "entity.other.keyframe-offset",
      ],
      "settings": {
        "foreground": "#acc59f"
      }
    },
    {
      // property name
      "scope": [
        // html attribute name
        "entity.other.attribute-name",
        "constant.character.escape", // `(\n)(\t)`
        "support.type.custom-property",
        // html deprecated attribute: border/align
        "invalid.deprecated.entity.other.attribute-name",
        // @supports (display: (grid)) {,normal,center,none,inline-block,fixed
        "support.constant.property-value",
      ],
      "settings": {
        "foreground": "#c5975e"
      }
    },
    {
      // reset to default color
      "scope": [
        "meta.jsx.children",
        "keyword.operator", // .../||/&&/??/!/|/&/^/++/--/==/!=/></>=<=
        "constant.other.color",
        "storage.type.function.arrow", // =>
        "punctuation.separator.key-value", // object_key(:)value/str(:) string
        // string Uppercase(<)First(>)
        "punctuation.definition.typeparameters",
        "punctuation.section.function",
        // attr(()data-bid())
        "punctuation.definition.group.begin.less",
        "punctuation.definition.group.end.less",
        "punctuation.definition.parameters.begin.bracket.round.css",
        "punctuation.definition.parameters.end.bracket.round.css",
        // scss ([) xxx=xxx (])
        "punctuation.definition.attribute-selector.begin.bracket.square.scss",
				"punctuation.definition.attribute-selector.end.bracket.square.scss",
      ],
      "settings": {
        "foreground": "#cccccc"
      }
    },
    {
      // css property name
			"scope": [
				"source.coffee.embedded",
				"support.type.property-name.css",
				"support.type.property-name.less",
        "support.type.vendor-prefix.less", // -webkit-/-moz-/-ms-/-o-
        "support.type.vendored.property-name.css",
        "meta.property-value.css",
        "entity.other.attribute-name.scss",
        "keyword.other.keyframe-selector.less",
        "support.type.property-name.media", // @media ((min-width): 100px)
        "meta.at-rule.media.header.css", // @media ((prefers-color-scheme): dark) {
        "entity.name.function.scss", // @keyframe (Animate)
        "meta.at-rule.keyframe.less",
			],
			"settings": {
				"foreground": "#b8a965"
			}
    },
    {
      "scope": [
        // undefined/null
        "constant.language.null",
        "constant.language.undefined",
        "keyword.operator.or.regexp", // (|)
        "keyword.control.anchor.regexp",
        "constant.language.import-export-all.ts",
        "punctuation.definition.character-class.regexp",
      ],
      "settings": {
        "foreground": "#d28080"
      }
    },
    {
      "scope": [
        // html tag name
        "entity.name.tag",
        "support.class.component", // jsx class component
        // json property name
        "support.type.property-name.json",
      ],
      "settings": {
        "foreground": "#dd636d"
      }
    },
    {
      // language keywords
      "scope": [
        "keyword",
        // const/var/let/function/interface/enum/class/(type Aa = 1)/constructor/get/set
        "storage.type",
        // import/export/(import type Aa)/return/if/else/for/while/switch/try/catch/finally/throw
        "keyword.control",
        // extends/class readonly/class private/class public
        "storage.modifier",
        // new
        "keyword.operator.new",
        "storage.type.class.jsdoc",
        // delete/void/typeof/instanceof/in
        "keyword.operator.expression",
        // module.exports
        "support.type.object.module.js",
        // template expression: ${}
        "punctuation.definition.template-expression",
      ],
      "settings": {
        "foreground": "#dd636d"
      }
    },
    {
      "scope": [
        "type.identifier",
        "constant.other.character-class.regexp" // (\d)
      ],
      "settings": {
        "foreground": "#707cbb"
      }
    },
    {
      // regexp: ?: ?=
      "scope": "punctuation.definition.group",
      "settings": {
        "foreground": "#C4704F"
      }
    },
    {
      "scope": [
        "support.function",
        "entity.name.function", // function name
        "variable.language.super", // (super)()
        // from vscode: https://github.com/microsoft/vscode/blob/d0857f8ec9655f17920cece120cde6be19f1ba2e/extensions/theme-defaults/themes/dark_plus.json#L11
        "support.constant.handlebars",
				"entity.name.operator.custom-literal",
				"source.powershell variable.other.member",
      ],
      "settings": {
        "foreground": "#68ade5"
      }
    },
    {
      "scope": [
        "support.class",
        "entity.name.type.class", // class (Name)
        "entity.name.type.module", // extends (React).Component
        "entity.other.inherited-class", // class Name extends (Test)
      ],
      "settings": {
        "foreground": "#8287d7"
      }
    },
    {
      "scope": [
        "support.type",
				"entity.name.type",
				"entity.name.type.parameter", // TS generic type
				// ts function params type
				"meta.function,meta.type.annotation",
				"entity.name.type.module,meta.namespace.declaration",
      ],
      "settings": {
        "foreground": "#54b5c6"
      }
    },
    {
      "scope": [
				// ts: string/number/boolean/symbol
        "support.type.primitive",
        "keyword.operator.quantifier.regexp",
      ],
      "settings": {
        "foreground": "#4bbda6"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "foreground": "#059669"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#5d99a9"
      }
    },
    {
      // link/image
      "scope": [
        "markup.underline.link",
        "variable.other.link.underline",
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#83d0b6d1"
      }
    },

    // git diff
    {
			"scope": [
        "markup.inserted",
        // "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
			"settings": {
				"foreground": "#85e89d"
			}
		},
		{
			"scope": [
        "markup.deleted",
        "punctuation.definition.deleted"
      ],
			"settings": {
				"foreground": "#cb7676"
			}
		},
		{
			"scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
			"settings": {
				"foreground": "#ffab70"
			}
		},
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#2f363d"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#b392f0",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#79b8ff"
      }
    },
  ]
}
