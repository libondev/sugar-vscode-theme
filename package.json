{
  "publisher": "banlify",
  "name": "sugar-vscode-theme",
  "displayName": "Sugar Theme",
  "type": "module",
  "version": "0.3.7",
  "description": "Sugar theme for Visual Studio Code",
  "author": "Libon <bon.li@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/libondev/sugar-vscode-theme#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/libondev/sugar-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/libondev/sugar-vscode-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "icon": "./logo.png",
  "engines": {
    "vscode": "^1.40.0"
  },
  "contributes": {
    "themes": [
      {
        "label": "Sugar Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/sugar-dark-color-theme.json"
      },
      {
        "label": "Sugar Dark Stylization",
        "uiTheme": "vs-dark",
        "path": "./themes/sugar-dark-stylization-color-theme.json"
      },
      {
        "label": "Sugar Light",
        "uiTheme": "vs",
        "path": "./themes/sugar-light-color-theme.json"
      },
      {
        "label": "Sugar Light Stylization",
        "uiTheme": "vs",
        "path": "./themes/sugar-light-stylization-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package --no-dependencies"
  }
}
