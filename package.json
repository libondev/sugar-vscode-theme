{
  "name": "sugar-vscode-theme",
  "displayName": "Sugar Theme",
  "type": "module",
  "version": "0.22.0",
  "description": "Sugar theme for Visual Studio Code",
  "publisher": "banlify",
  "author": "Libon <bon.li@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/libondev/sugar-vscode-theme#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/libondev/sugar-vscode-theme"
  },
  "bugs": {
    "url": "https://github.com/libondev/sugar-vscode-theme/issues"
  },
  "categories": [
    "Themes"
  ],
  "icon": "./logo.png",
  "engines": {
    "vscode": "^1.40.0"
  },
  "scripts": {
    "dev": "tsx --watch ./src/gen.ts",
    "build": "vsce package --no-dependencies",
    "prebuild": "tsx ./src/gen.ts"
  },
  "contributes": {
    "themes": [
      {
        "label": "Sugar Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark.json"
      },
      {
        "label": "Sugar Light",
        "uiTheme": "vs",
        "path": "./themes/light.json"
      },
      {
        "label": "Sugar Dark Focus",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-focus.json"
      },
      {
        "label": "Sugar Light Focus",
        "uiTheme": "vs",
        "path": "./themes/light-focus.json"
      },
      {
        "label": "Sugar Dark Vitesse",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-vitesse.json"
      },
      {
        "label": "Sugar Light Vitesse",
        "uiTheme": "vs",
        "path": "./themes/light-vitesse.json"
      },
      {
        "label": "Sugar Light VS",
        "uiTheme": "vs",
        "path": "./themes/light-vs.json"
      },
      {
        "label": "Sugar Light Github",
        "uiTheme": "vs",
        "path": "./themes/light-github.json"
      },
      {
        "label": "Sugar Dark VS",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-vs.json"
      },
      {
        "label": "Sugar Dark Fleet",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-fleet.json"
      },
      {
        "label": "Sugar Dark Github",
        "uiTheme": "vs-dark",
        "path": "./themes/dark-github.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^22.2.0",
    "@vscode/vsce": "^3.3.2",
    "tsx": "^4.17.0"
  }
}
